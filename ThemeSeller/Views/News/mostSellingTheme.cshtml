@model IEnumerable<DataLayer.Theme>
    
<section class="last-news-side border-radius">
    <header><i class="icons icon-star"></i> <h3> پربازدیدترین اخبار </h3></header>
    <ul>

        @foreach (var item in Model)
        {
            <li>
                <img class="border-radius" src="/ThemeImages/@item.ImageName">
                <h2> <a href="/News/@item.ThemeID">@item.ThemeID</a></h2>
                <span>انتشار : @item.CreateDate.ToShamsi()</span>
            </li>
        }

    </ul>
</section>
